@model Tractor


@{
    ViewData["Title"] = "Index";
}

<div style="background-color:palevioletred; margin-left:100px;">
    <p>@Model.Id</p>
    <p>@Model.ViewTranslations.Culture</p>
    <p>@Model.ViewTranslations.Slug</p>
    <p>@Model.Year</p>
    <p>@Model.Model</p>
    <p>@Model.ViewTranslations.Description</p>
</div>


